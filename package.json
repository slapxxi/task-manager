{
  "name": "react-template",
  "version": "1.0.0",
  "description": "Template for a typical React application",
  "main": "src/index.js",
  "scripts": {
    "build": "rimraf dist && webpack --config=webpack.config.prod.js",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "csscomb": "csscomb --tty-mode -c .csscomb.json",
    "eslint": "eslint",
    "prettier": "prettier --write",
    "commit": "git stash -k && git commit && git stash pop",
    "start": "webpack-dev-server --config=webpack.config.dev.js",
    "svgo": "svgo",
    "test:debug:win": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand --watch",
    "test:watch": "jest --watch",
    "test": "jest",
    "tslint": "tslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "react",
    "template"
  ],
  "author": "Slava Pavlutin",
  "license": "MIT",
  "dependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/jest": "^23.3.9",
    "@types/lodash": "^4.14.118",
    "@types/node": "^10.12.5",
    "@types/react": "^16.7.2",
    "@types/react-dom": "^16.0.8",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-test-renderer": "^16.0.2",
    "@types/styled-components": "^4.0.3",
    "@types/uuid": "^3.4.4",
    "@types/webpack": "^4.4.18",
    "date-fns": "^1.29.0",
    "firebase": "^5.5.8",
    "lodash": "^4.17.11",
    "polished": "^2.3.0",
    "popmotion": "^8.5.0",
    "react": "^16.7.0-alpha.0",
    "react-day-picker": "^7.2.4",
    "react-dom": "^16.7.0-alpha.0",
    "react-pose": "^4.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.0.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.3.1",
    "babel": "^6.23.0",
    "babel-eslint": "^9.0.0",
    "css-loader": "^1.0.1",
    "csscomb": "^4.2.0",
    "docz": "^0.11.2",
    "dotenv": "^6.1.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.26.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-styled-components": "^6.3.0",
    "lint-staged": "^7.3.0",
    "mini-css-extract-plugin": "^0.4.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.15.2",
    "react-test-renderer": "^16.6.1",
    "react-testing-library": "^5.2.3",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.1",
    "svg-sprite-loader": "^3.9.2",
    "svgo": "^1.1.1",
